@model AutomotoraWeb.Models.SeniaModel
@using AutomotoraWeb.Controllers.General;
@using DLL_Backend;
@using AutomotoraWeb.Models;
@using AutomotoraWeb.Helpers;




<div style="width: 1200px">
    <div class="selection_form">
        <div class="floatLeft" style="margin-right: 10px">
            <div class="text">
                Seleccionados:
            </div>
            <div>
                @Html.DevExpress().ListBox(
            settings => {
                settings.Name = "SelectedRows";
                settings.Width = 120;
                settings.Height = 200;
                settings.Properties.EnableClientSideAPI = true;
            }).GetHtml()
            </div>
            <div class="text">
                Cantidad:&nbsp;<strong id="count">0</strong>
            </div>
            <div class="clear-both"></div>
        </div>
        <div class="floatLeft" id="divGrillaCheques">
            @Html.Partial("_grillaChequesDevolucion", Model.SeniaDev.Pago.Cheques)
        </div>
    </div>
</div>


