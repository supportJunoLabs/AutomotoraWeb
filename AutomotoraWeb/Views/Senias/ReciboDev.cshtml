@model DLL_Backend.TRSeniaDevolucion 
@using AutomotoraWeb.Controllers.Sales;

@{  Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Recibo Devolucion Seña";
}

@section HeadResources{
    @Html.Partial("_devExpressReporte")
}

<span class="validation-summary-errors">@ViewBag.ErrorMessage</span>

@{ if (ViewData["idParametros"]==null || (int)ViewData["idParametros"]==0) { return; }}

<div class="btn-toolbar">
    @(Html.Bootstrap().ActionLinkButton("Consulta Seña", "details", SeniasController.CONTROLLER).Class("btn btn-default").RouteValues(new{id=Model.Senia.Codigo}))
</div>
<div class="clear-both"></div>


@Html.Partial("_reciboDev")