    </div>

    <div class="clear-both"></div>

    <div>
        <div class="dataLine">
            <div class="display-label-90 ">@Html.LabelFor(model => model.Observaciones)</div>
            <div class="editor-field editor-field-xxlarge">
                <textarea id="vehiculoObservaciones" style="width: 725px; height: 60px"  disabled="disabled">@Model.Observaciones</textarea></div>
            <div class="clear-both"></div>
        </div>
    </div>
