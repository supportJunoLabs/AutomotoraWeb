        </div>
        @*<div class="btn-group">
            <a class="btn btn-primary2-mf" title="Imprimir listado" href='@Url.Action("Create", "Vendedores")'>
                Imprimir</a>
        </div>*@
    </div>
       
    <div id="clientesGrid">
        @Html.Partial("_listCustomers", Model)
