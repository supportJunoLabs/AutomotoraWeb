@model DLL_Backend.Venta
@using AutomotoraWeb.Controllers.General;
@using DLL_Backend;
@using AutomotoraWeb.Models;

@{ 
    bool modoConsulta = ((bool?)ViewBag.SoloLectura) ?? false;
    bool cambiarSucursal = ViewBag.Multisucursal; 
}

<div class="well" style="margin-top: 10px; width: 1024px;">
    <ul class="nav nav-tabs">
        <li class="active"><a href="#vehiculo_tab" data-toggle="tab">Vehiculo</a></li>
        <li><a href="#condicionesVenta_tab" data-toggle="tab">Condiciones de Venta</a></li>
        <li><a href="#metodosPago_tab" data-toggle="tab">Métodos de Pago</a></li>
        <li><a href="#seniaAnticipos_tab" data-toggle="tab">Seña y Anticipos</a></li>
    </ul>
    <div id="tabContentVenta" class="tab-content" style="height: 920px;">
        <div class="tab-pane active in" id="vehiculo_tab">
            @{if (Model.Vehiculo != null && Model.Vehiculo.Codigo > 0) {
                @Html.Partial("_seleccionDeVehiculosDetalle", Model.Vehiculo)
              }}
        </div>
        <div class="tab-pane fade" id="condicionesVenta_tab">
            @Html.Partial("_condicionesVenta", Model)
        </div>
        <div class="tab-pane fade" id="metodosPago_tab">
            @Html.Partial("_ventaMetodosDePago", Model)
        </div>
        <div class="tab-pane fade" id="seniaAnticipos_tab">
            @Html.Partial("_ventaSeniaAnticipos", Model)
        </div>
     </div>
</div>


<script type="text/javascript">
 

</script>
