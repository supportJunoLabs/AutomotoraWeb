@model DLL_Backend.ChequeEmitido
@using AutomotoraWeb.Helpers;
@using AutomotoraWeb.Controllers.General;


@{
    ViewBag.Title = ViewBag.NombreEntidad + ": Agregar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<span class="validation-summary-errors">@ViewBag.ErrorMessage</span>

@using (Html.BeginForm(null, null, FormMethod.Post,new { id="formPrincipal" })) {
    @Html.ValidationSummary(true)
    @Html.ValidationSummary("Se han producido errores:")
   
    
    <div class="espacio" />
    
    <fieldset>
        <legend></legend>

        @Html.Partial("_datosDetalle", Model)

    </fieldset>
    
    <div class="espacio2"></div>
    <div class="btn-toolbar">
        @(Html.Bootstrap().Button().Class("btn btn-primary icon-save").Text(" Guardar").Id("btn_guardar"))
        @(Html.Bootstrap().ActionLinkButton("Cancelar", BaseController.SHOW).Class("btn btn-default"))
    </div>
    
  }

<script src="~/Scripts/AutomotoraWeb/ChEmitido.js"></script>
<script src="~/Scripts/AutomotoraWeb/ChEmitidoCreateModif.js"></script>
