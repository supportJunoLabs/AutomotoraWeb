@using AutomotoraWeb.Controllers.Sistema;
@{
    Layout = null;  
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Error 403</title>
    @Styles.Render("~/Content/css") @*Bootstrap awesome y template propio*@
</head>
<body class="http-error">
    <div class="row-fluid">
        <div class="http-error">
            <h1>Oops!</h1>
            <p class="info">
                No posee permisos para acceder a esta funcionalidad.
            </p>
            <p>
                <i class="icon-home"></i>
            </p>
            @(Html.Bootstrap().ActionLinkButton("Volver al inicio",SistemaController.INDEX, SistemaController.BCONTROLLER).Class("btn btn-default"))
        </div>
    </div>
</body>
</html>