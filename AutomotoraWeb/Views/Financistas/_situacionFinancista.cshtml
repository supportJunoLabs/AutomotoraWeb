@model AutomotoraWeb.Models.SituacionFinancista
@using AutomotoraWeb.Helpers;


<div>
    Importe Total: @Html.DisplayFor(m=>m.TotalGeneral.ImporteTexto)
</div>

<div class="well" style="margin-top: 25px; width: 1300px; height: 1100px;">
    <ul class="nav nav-tabs">
        <li class="active"><a href="#cuotas_tab" data-toggle="tab">Cheques</a></li>
        <li><a href="#vales_tab" data-toggle="tab">Efectivo</a></li>
    </ul>
    <div id="myTabContent" class="tab-content">
        <div class="tab-pane active in" id="cuotas_tab">
            @Html.Partial("_sitFinancistaCheques", Model)
        </div>
        <div class="tab-pane fade" id="vales_tab">
             @Html.Partial("_sitFinancistaEfectivo", Model)
        </div>
    </div>
</div>