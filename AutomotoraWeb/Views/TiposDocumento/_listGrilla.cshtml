@model List<DLL_Backend.TipoDocumento>
@using System.Web.UI.WebControls;
@using AutomotoraWeb.Helpers;
@using AutomotoraWeb.Controllers.Sales.Maintenance;

@{
    List<string> hiddenColumns = new List<string>() {  "Codigo"   }; // En esta lista hay que agregar las columnas que no se quieran mostrar en la grilla
    ViewBag.hiddenColumns = hiddenColumns;

    Dictionary<string, int> trunkColumns = new Dictionary<string, int>() { { "Nombre", 30 }, { "Observaciones", 50 }};
    ViewBag.trunkColumns = trunkColumns;

    ViewBag.nameGrid = "grilla";
    ViewBag.controller = TiposDocumentoController.CONTROLLER;
    ViewBag.actionCallbackRoute = "listaGrilla";
    ViewBag.KeyFieldName = "Codigo";
    ViewBag.typeOfModel = typeof(DLL_Backend.TipoDocumento.TipoDocumentoMD);
}

@Html.Partial("_generalGrid", Model)
  
