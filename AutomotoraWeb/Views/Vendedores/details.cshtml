@model AutomotoraWeb.Models.VendedorModel
@using AutomotoraWeb.Controllers.General;

@{
    ViewBag.Title =ViewBag.NombreEntidad+": Consulta";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section HeadResources{
    @Html.Partial("_devExpressGrilla")
}

<span class="validation-summary-errors">@ViewBag.ErrorMessage</span>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    @Html.ValidationSummary("Se han producido errores:")

    <fieldset>
        <legend></legend>
        @Html.Partial("_datosDetalle", Model)

    </fieldset>
    
    <div class="btn-toolbar">
        @(Html.Bootstrap().ActionLinkButton("Modificar", BaseController.EDIT).RouteValues(new { id = Model.Vendedor.Codigo }).Class("btn btn-primary").Title("Editar Vendedor"))
        @(Html.Bootstrap().ActionLinkButton("Eliminar", BaseController.DELETE).RouteValues(new { id = Model.Vendedor.Codigo }).Class("btn btn-primary").Title("Eliminar Vendedor"))
        @(Html.Bootstrap().ActionLinkButton("Volver", BaseController.SHOW).Class("btn btn-default").Title("Volver a Lista de Vendedores"))
    </div>

}


@section Scripts {
}

