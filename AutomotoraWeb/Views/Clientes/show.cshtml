@model List<DLL_Backend.Cliente>
@using AutomotoraWeb.Controllers.General;

@{
    ViewBag.Title = "Clientes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row-fluid">
    <div class="btn-toolbar">
        <div class="btn-group">
            @(Html.Bootstrap().ActionLinkButton("Agregar Cliente", "Create").AppendIcon(TwitterBootstrapMVC.Icons.plus, true).Style(TwitterBootstrap2.ButtonStyle.Primary).Title("Agregar Cliente"))
        )
        </div>
         <div class="btn-group">
            <a class="btn btn-primary2-mf" title="Imprimir listado" href='@Url.Action(BaseController.REPORT)'>Imprimir Listado</a>
        </div>
    </div>
       
    <div id="clientesGrid">
        @Html.Partial("_listClientes", Model)
    </div>
</div>



