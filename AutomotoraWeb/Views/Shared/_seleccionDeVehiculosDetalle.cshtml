            <div class="clear-both"></div>
        </div>
      
       
    </div>

    <div class="floatLeft">
        <div class="dataLine">
            <div class="display-label-90">@Html.LabelFor(model => model.Sucursal)</div>
            <div class="editor-field editor-field-large renglon-alto"><input id="vehiculoSucursal" value='@Model.Sucursal' disabled="disabled" /></div>
            <div class="clear-both"></div>
        </div>
         <div class="dataLine">
            <div class="display-label-90">@Html.LabelFor(model => model.PrecioVenta)</div>
