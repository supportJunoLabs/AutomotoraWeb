@model DLL_Backend.Cliente
@using DLL_Backend;
@using AutomotoraWeb.Controllers.General;

@{ Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Situacion Cliente";
}
@section HeadResources{
    @Html.Partial("_devExpressReporte")
}
<span class="validation-summary-errors">@ViewBag.ErrorMessage</span>

<div class="btn-toolbar">
    @(Html.Bootstrap().ActionLinkButton("Volver", "SitCliente").Class("btn btn-default").RouteValues(new{id=Model.Codigo}))
</div>
<div class="clear-both"></div>

@Html.Partial("_reportSitCliente", Model.Codigo)