@model List<AutomotoraWeb.Models.SellerModel>
@using System.Web.UI.WebControls
@using AutomotoraWeb.Helpers;

@{
    List<string> hiddenColumns = new List<string>() { "Photo" }; // En esta lista hay que agregar las columnas que no se quieran mostrar en la grilla
    ViewBag.hiddenColumns = hiddenColumns;

    ViewBag.nameGrid = "gridSellers";
    ViewBag.controller = "sellers";
    ViewBag.actionCallbackRoute = "listSellers";
    ViewBag.KeyFieldName = "Id";
    ViewBag.typeOfModel = typeof(AutomotoraWeb.Models.SellerModel);
}

@Html.Partial("_generalGrid", Model)
  
