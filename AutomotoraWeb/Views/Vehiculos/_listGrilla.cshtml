@model List<DLL_Backend.Vehiculo>
@using System.Web.UI.WebControls;
@using AutomotoraWeb.Helpers;
@using AutomotoraWeb.Controllers.Sistema;

@{
    List<string> hiddenColumns = new List<string>() { "Codigo", "Padron", "Chasis", "Costo", "FechaAdquirido", "PrecioVenta", "Departamento" }; // En esta lista hay que agregar las columnas que no se quieran mostrar en la grilla
    ViewBag.hiddenColumns = hiddenColumns;

    Dictionary<string, int> trunkColumns = new Dictionary<string, int>() { };
    ViewBag.trunkColumns = trunkColumns;
    
    ViewBag.nameGrid = "grilla";
    ViewBag.controller = AutomotoraWeb.Controllers.Sales.Maintenance.VehiculosController.CONTROLLER;
    ViewBag.actionCallbackRoute = "listaGrilla";
    ViewBag.KeyFieldName = "Codigo";
    ViewBag.typeOfModel = typeof(DLL_Backend.Vehiculo.VehiculoMD);
}

@Html.Partial("_generalGrid", Model)
  
