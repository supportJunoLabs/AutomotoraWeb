@model List<DLL_Backend.CuentaBancaria>
@using System.Web.UI.WebControls;
@using AutomotoraWeb.Helpers;
@using AutomotoraWeb.Controllers.Bank.Maintenance;
@using AutomotoraWeb.Models;

@{
    List<string> visibleColumns = new List<string>() { "Banco", "Numero", "Tipo", "Moneda"};
    Dictionary<string, int> trunkColumns = new Dictionary<string, int>() { { "Banco", 30 }, { "Numero", 30 }, { "Tipo", 30 }, { "Moneda", 30 } };
    
    InfoGrilla ig = new InfoGrilla();
    ig.VisibleColumns=visibleColumns;
    ig.TrunkColumns = trunkColumns;
    ig.NameGrid = "grilla";
    ig.Controller = CuentasBancariasController.CONTROLLER;
    ig.ActionCallbackRoute = "listaGrilla";
    ig.KeyFieldName = "Codigo";
    ig.TypeOfModel = typeof(DLL_Backend.CuentaBancaria.CuentaBancariaMD);
    
    ViewBag.InfoGrilla = ig;
}

@Html.Partial("_generalGrid", Model)
  
