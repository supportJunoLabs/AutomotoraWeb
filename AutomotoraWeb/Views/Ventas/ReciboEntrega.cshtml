@model DLL_Backend.Entrega
@using AutomotoraWeb.Controllers.Sales;
@using AutomotoraWeb.Controllers.General;

@{  Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Entrega Vehiculo";
}
@section HeadResources{
    @Html.Partial("_devExpressReporte")
}

<span class="validation-summary-errors">@ViewBag.ErrorMessage</span>

@{ if (ViewData["idParametros"]==null || (int)ViewData["idParametros"]==0) { return; }}

<div class="btn-toolbar">
    @(Html.Bootstrap().ActionLinkButton("Consulta Venta", "Details", VentasController.CONTROLLER).Class("btn btn-default").RouteValues(new{id=Model.Venta.Codigo}))
</div>
<div class="clear-both"></div>


@Html.Partial("_reciboEntrega")