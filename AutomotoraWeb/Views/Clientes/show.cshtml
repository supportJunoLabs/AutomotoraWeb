@model List<DLL_Backend.Cliente>
@using AutomotoraWeb.Controllers.General;

@{
    //ViewBag.Title = @Html.Action("NombreEntidades").ToString();
    ViewBag.Title  = ViewBag.NombreEntidades;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section HeadResources{
    @Html.Partial("_devExpressGrilla")
}


<span class="validation-summary-errors">@ViewBag.ErrorMessage</span>

<div class="row-fluid">
    <div class="btn-toolbar">
        <div class="btn-group">
            @(Html.Bootstrap().ActionLinkButton("Agregar ", "Create").AppendIcon(TwitterBootstrapMVC.Icons.plus, true).Style(TwitterBootstrap2.ButtonStyle.Primary))
        )
        </div>
         <div class="btn-group">
            <a class="btn btn-primary2-mf" title="Listado" href='@Url.Action(BaseController.REPORT)'>Listado</a>
        </div>
    </div>
       
    <div id="divGrilla">
        @Html.Partial("_listGrilla", Model)
    </div>
</div>



