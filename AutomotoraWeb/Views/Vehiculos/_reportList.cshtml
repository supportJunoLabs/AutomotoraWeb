@model AutomotoraWeb.Models.ListadoVehiculosModel
@using AutomotoraWeb.Controllers.General;
@using AutomotoraWeb.Controllers.Sales.Maintenance;


@Html.DevExpress().DocumentViewer(settings =>{
    // The following settings are required for a Report Viewer. 
    settings.Name = "documentViewer";
    settings.Report = (DevExpress.XtraReports.UI.XtraReport)ViewData["Report"];
    // Callback and export route values specify corresponding controllers and their actions. 
    // These settings are required as well. 
    settings.CallbackRouteValues = new { Controller = VehiculosController.CONTROLLER, Action = BaseController.REPORT_PARTIAL, id=Model.id };
    settings.ExportRouteValues = new { Controller = VehiculosController.CONTROLLER, Action = BaseController.REPORT_EXPORT, id=Model.id  };
}).GetHtml()
