@model List<DLL_Backend.Cliente>
@using System.Web.UI.WebControls
@using AutomotoraWeb.Helpers;

@{
    List<string> hiddenColumns = new List<string>() { }; // En esta lista hay que agregar las columnas que no se quieran mostrar en la grilla
    ViewBag.hiddenColumns = hiddenColumns;

    List<string> visibleColumns = new List<string>() { "Nombre", "Telefono", "OtroTelefono", "Fax", "Observaciones" }; //agregar lo que hay que mostrar, si queda vacio se muestra todo
    ViewBag.visibleColumns = visibleColumns;
    
    ViewBag.nameGrid = "gridClientes";
    ViewBag.controller = "clientes";
    ViewBag.actionCallbackRoute = "listClientes";
    ViewBag.KeyFieldName = "Codigo";
    ViewBag.typeOfModel = typeof(DLL_Backend.Cliente.ClienteMD);
}

@Html.Partial("_generalGrid", Model)

