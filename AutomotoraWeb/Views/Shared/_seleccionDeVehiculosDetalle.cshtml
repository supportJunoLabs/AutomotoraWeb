@model DLL_Backend.Vehiculo
@using AutomotoraWeb.Helpers;
@using DLL_Backend;

<style type="text/css">
    .estiloInput {
         border-radius:4px;
         height:30px;
    }

</style>

<div>
    <div class="floatLeft" style="margin-right: 110px;">
        <div class="dataLine">
            <div class="display-label-90">@Html.LabelFor(model => model.Ficha)</div>
            <div class="editor-field editor-field-large renglon-alto">
                <input id="vehiculoFicha" value= '@Model.Ficha' disabled="disabled"  /></div>
           
            <div class="clear-both"></div>
        </div>
        <div class="dataLine">
            <div class="display-label-90">@Html.LabelFor(model => model.Marca)</div>
            <div class="editor-field editor-field-large renglon-alto">
                <input id="vehiculoMarca" value='@Model.Marca' disabled="disabled" /></div>
            <div class="clear-both"></div>
        </div>
          <div class="dataLine">
            <div class="display-label-90">@Html.LabelFor(model => model.Modelo)</div>
            <div class="editor-field editor-field-large renglon-alto"><input id="vehiculoModelo" value='@Model.Modelo' disabled="disabled" /></div>
