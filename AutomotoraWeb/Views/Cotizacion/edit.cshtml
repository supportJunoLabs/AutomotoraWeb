@model List<DLL_Backend.Moneda>
@using DLL_Backend;

@{
    ViewBag.Title = "Cotizaciones";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section HeadResources{
    @Html.Partial("_devExpressGrilla")
}

<span class="validation-summary-errors">@ViewBag.ErrorMessage</span>

<div>
    Cotizaciones en relacion a 1 @Moneda.MonedaDefault.Nombre
</div>
<div class="espacio"></div>

<div class="row-fluid">
       <div id="divGrilla">
        @Html.Partial("_grillaCotizaciones", Model)
    </div>
</div>


