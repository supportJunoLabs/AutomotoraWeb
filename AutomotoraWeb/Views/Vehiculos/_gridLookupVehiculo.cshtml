
@Html.DevExpress().GridLookup(settings => {
    settings.Name = "gridLookupVehiculos";
    settings.GridViewProperties.CallbackRouteValues = new { Controller = "Vehiculos", Action = "GridLookupVehiculo" };
    settings.Columns.Add("Codigo");
    settings.Columns.Add("Chasis");
    settings.Columns.Add("Anio");

    settings.KeyFieldName = "Codigo";
    
}).BindList(ViewBag.ListVehiculos).GetHtml()
